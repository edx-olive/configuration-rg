# This file was generated by Ansible for {{ansible_fqdn}}
# Do NOT modify this file by hand!

{{ backup_logdir }}/*.log {
{% if backup_runas_user != 'root' %}
    su {{ backup_runas_user }} root
{% endif %}
    compress
    copytruncate
    notifempty
    dateext
    missingok
    rotate 10
    size 10M
}
